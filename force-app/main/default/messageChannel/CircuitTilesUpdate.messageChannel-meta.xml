<?xml version="1.0" encoding="UTF-8" ?>
<LightningMessageChannel xmlns="http://soap.sforce.com/2006/04/metadata">
    <masterLabel>CircuitTilesUpdate</masterLabel>
    <isExposed>true</isExposed>
    <description>This message is fired when the circuit tiles are updated.</description>
    <lightningMessageFields>
        <fieldName>circuitId</fieldName>
        <description>The ID of the circuit.</description>
    </lightningMessageFields>
    <lightningMessageFields>
        <fieldName>Location</fieldName>
        <description>The location of the circuit.</description>
    </lightningMessageFields>
    <lightningMessageFields>
        <fieldName>Length</fieldName>
        <description>The length of the circuit.</description>
    </lightningMessageFields>
    <lightningMessageFields>
        <fieldName>Turns</fieldName>
        <description>The number of turns in the circuit.</description>
    </lightningMessageFields>
    <lightningMessageFields>
        <fieldName>Surface</fieldName>
        <description>The surface type of the circuit.</description>
    </lightningMessageFields>
    <lightningMessageFields>
        <fieldName>StartYear</fieldName>
        <description>The starting year of the circuit.</description>
    </lightningMessageFields>
    <lightningMessageFields>
        <fieldName>Latitude</fieldName>
        <description>Circuit Latitude Number.</description>
    </lightningMessageFields>
    <lightningMessageFields>
        <fieldName>Longitude</fieldName>
        <description>Circuit Longitude Number.</description>
    </lightningMessageFields>
    <lightningMessageFields>
        <fieldName>IsActive</fieldName>
        <description>Check if the record is selected.</description>
    </lightningMessageFields>
</LightningMessageChannel>
